<div class="field">
  <%= f.attribute_fields do |a| %>
    <%= a.attribute_select associations: [:evidence_source, :software_engineering_methods, :software_engineering_methodologies] %>
  <% end %>
  <%= f.predicate_select %>
  <%= f.value_fields do |v| %>
    <%= v.text_field :value %>
  <% end %>
  <%= link_to "remove", '#', class: "remove_fields" %>
</div>